{
  "name": "gemfire-node-sample",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "express": "^4.16.3",
    "node-pre-gyp": "^0.10.0",
    "morgan": "^1.8.1",
    "rimraf": "^2.6.1"
  },
  "optionalDependencies": {
    "gemfire": "^1.0.1"
  },
  "scripts": {
